list = []
i=0
while len(list)<10000:
    delitel = 0
    i+=1
    for ii in range(i):    # делим число i на числа от 0 и доо числа i
        
        try:
            if i%ii==0:     #если делится без остатка добавляем к количеству делителей 1
                delitel+=1
            else:
                continue
            
        except:
            continue
          
    if delitel==1:
            list.append(i)
            
print(list)
print(len(list))
print(list[-1])  
